
<script src="https://www.google.com/recaptcha/api.js?render=6LcFQqgZAAAAAKjvG3DAACEUnb62n2qd44UFiBWX"></script>
<script src="https://js.stripe.com/v3/"></script>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title></title>
</head>
<body>
    <div>
        <div class="col-sm-4">
            <h3>Log In</h3>
            <form data-bind="submit: login">
                <div class="form-group">
                    <label>Email</label>
                    <input class="form-control" type="text" data-bind="value: loginEmail" />
                </div>
                <div class="form-group">
                    <label>Password</label>
                    <input class="form-control" type="password" data-bind="value: loginPassword" />
                </div>
                <div class="form-group">
                    <button type="submit" class="btn btn-default">Log In</button>

                </div>
            </form>
        </div>
    </div>
</body>
</html>

<style>
    .btn-default {
        float: right;
        margin-right: 70px;
    }
</style>
